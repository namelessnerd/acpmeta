<?xml version="1.0"?>
<rdf:RDF
    xmlns:spinmap="http://spinrdf.org/spinmap#"
    xmlns:iPol="http://ontology.techlabs.acn.com/IntelligentInfrastructurePolicy#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:topLevelPolicy="http://ontology.techlabs.acn.com/topLevelPolicy#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:appModel="http://ontology.techlabs.acn.com/appModel#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:cloudModel="http://ontology.techlabs.acn.com/cloudProviderModel#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://ontology.techlabs.acn.com/IntelligentInfrastructurePolicy">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://ontology.techlabs.acn.com/infraModel"/>
    <owl:imports rdf:resource="http://ontology.techlabs.acn.com/appModel"/>
    <owl:imports rdf:resource="http://ontology.techlabs.acn.com/cloudProviderModel"/>
    <owl:imports rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="InfrastructureWebServerSecurityPolicy">
    <rdfs:subClassOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#Policy"/>
  </owl:Class>
  <owl:Class rdf:ID="InfrastructureDBServerPolicyConstraint">
    <owl:equivalentClass rdf:resource="http://ontology.techlabs.acn.com/appModel#SecurityModule"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="InfrastructurePolicyConstraint"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="InfrastructureWebServerPolicyArtifact">
    <owl:equivalentClass rdf:resource="http://ontology.techlabs.acn.com/appModel#WebServer"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="InfrastructurePolicyArtifact"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="InfrastructureDBServerPolicyArtifact">
    <owl:equivalentClass rdf:resource="http://ontology.techlabs.acn.com/appModel#Database"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#InfrastructurePolicyArtifact"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#InfrastructurePolicyConstraint">
    <rdfs:subClassOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#Constraint"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#InfrastructurePolicyArtifact"/>
          <rdf:Description rdf:about="http://ontology.techlabs.acn.com/topLevelPolicy#Logical"/>
          <rdf:Description rdf:about="http://ontology.techlabs.acn.com/topLevelPolicy#Quantitative"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#InfrastructurePolicyArtifact">
    <rdfs:subClassOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#Artifact"/>
  </owl:Class>
  <owl:Class rdf:ID="WebServerSecurityPolicyConstruct">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="InfrastructurePolicyConstruct"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="InfrastructureWebServerSecurityPolicyConstraint">
    <owl:equivalentClass rdf:resource="http://ontology.techlabs.acn.com/appModel#SecurityModule"/>
    <rdfs:subClassOf rdf:resource="#InfrastructurePolicyConstraint"/>
  </owl:Class>
  <owl:Class rdf:about="#InfrastructurePolicyConstruct">
    <rdfs:subClassOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#PolicyConstruct"/>
    <spin:rule>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >s</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >appModelMiddleware</sp:varName>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >appModelMiddleware</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
            <sp:object rdf:resource="http://ontology.techlabs.acn.com/appModel#Middleware"/>
          </rdf:Description>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >s</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object rdf:resource="#InfrastructurePolicyConstruct"/>
          </rdf:Description>
        </sp:templates>
      </sp:Construct>
    </spin:rule>
  </owl:Class>
  <owl:Class rdf:ID="InfrastructureMandatoryPolicyConstraint">
    <rdfs:subClassOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#Mandatory"/>
  </owl:Class>
  <owl:Class rdf:ID="DBServerPolicyConstruct">
    <rdfs:subClassOf rdf:resource="#InfrastructurePolicyConstruct"/>
  </owl:Class>
  <rdf:Property rdf:ID="hasInfrastructureWebServerSecurityCondition">
    <rdfs:range rdf:resource="#InfrastructureWebServerSecurityPolicyConstraint"/>
    <rdfs:domain rdf:resource="#WebServerSecurityPolicyConstruct"/>
    <rdfs:subPropertyOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#hasPolicyConstructCondition"/>
  </rdf:Property>
  <rdf:Property rdf:ID="hasAppArtifact">
    <rdfs:range rdf:resource="http://ontology.techlabs.acn.com/appModel#Middleware"/>
    <rdfs:domain rdf:resource="#InfrastructurePolicyArtifact"/>
  </rdf:Property>
  <rdf:Property rdf:ID="hasInfrastructurePolicyMandatoryConstraint">
    <rdfs:range rdf:resource="#InfrastructureMandatoryPolicyConstraint"/>
    <rdfs:subPropertyOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#hasPolicyConstructConstraint"/>
    <rdfs:domain rdf:resource="#InfrastructureWebServerSecurityPolicy"/>
  </rdf:Property>
  <rdf:Property rdf:ID="hasInfrastructureArtifact">
    <rdfs:range rdf:resource="http://ontology.techlabs.acn.com/appModel#WebServer"/>
    <rdfs:domain rdf:resource="#WebServerSecurityPolicyConstruct"/>
    <rdfs:subPropertyOf rdf:resource="http://ontology.techlabs.acn.com/topLevelPolicy#hasPolicyConstructArtifact"/>
    <rdfs:domain rdf:resource="#InfrastructurePolicyConstruct"/>
    <rdfs:range rdf:resource="#InfrastructurePolicyArtifact"/>
  </rdf:Property>
  <rdf:Property rdf:ID="hasWebServerSecurityPolicyArtifact">
    <rdfs:range rdf:resource="http://ontology.techlabs.acn.com/appModel#WebServer"/>
    <rdfs:domain rdf:resource="#WebServerSecurityPolicyConstruct"/>
    <rdfs:subPropertyOf rdf:resource="#hasInfrastructureArtifact"/>
  </rdf:Property>
  <rdf:Property rdf:ID="hasDBServerPolicyArtifact">
    <rdfs:range rdf:resource="#InfrastructureDBServerPolicyArtifact"/>
    <rdfs:domain rdf:resource="#DBServerPolicyConstruct"/>
    <rdfs:subPropertyOf rdf:resource="#hasInfrastructureArtifact"/>
  </rdf:Property>
  <iPol:InfrastructurePolicyArtifact rdf:ID="InfrastructurePolicyArtifact_"/>
  <iPol:InfrastructureWebServerSecurityPolicy rdf:ID="WebServerSecurityPolicy">
    <iPol:hasInfrastructureWebServerSecurityCondition rdf:resource="http://ontology.techlabs.acn.com/appModel#IISSSL"/>
    <iPol:hasInfrastructurePolicyMandatoryConstraint>
      <iPol:InfrastructureMandatoryPolicyConstraint rdf:ID="RequiresMandatoryConstraint"/>
    </iPol:hasInfrastructurePolicyMandatoryConstraint>
  </iPol:InfrastructureWebServerSecurityPolicy>
  <iPol:InfrastructurePolicyConstruct rdf:ID="ACMEBankIISPolicy">
    <topLevelPolicy:hasPolicyConstructConstraint rdf:resource="http://ontology.techlabs.acn.com/appModel#IISSSLSecurityModule"/>
    <topLevelPolicy:hasPolicyConstructCondition rdf:resource="#RequiresMandatoryConstraint"/>
    <iPol:hasInfrastructureArtifact rdf:resource="http://ontology.techlabs.acn.com/appModel#ACMEBankIIS"/>
  </iPol:InfrastructurePolicyConstruct>
  <iPol:InfrastructurePolicyConstruct rdf:ID="InfrastructureWebServerSecurityPolicy001">
    <topLevelPolicy:hasPolicyConstructConstraint rdf:resource="http://ontology.techlabs.acn.com/appModel#IISSSLSecurityModule"/>
    <topLevelPolicy:hasPolicyConstructCondition rdf:resource="#RequiresMandatoryConstraint"/>
    <iPol:hasInfrastructureArtifact rdf:resource="http://ontology.techlabs.acn.com/appModel#IIS"/>
  </iPol:InfrastructurePolicyConstruct>
  <rdf:Description rdf:about="http://ontology.techlabs.acn.com/appModel#Module">
    <spinmap:rule>
      <sp:Construct>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:resource="#InfrastructurePolicyConstraint"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
        </sp:templates>
        <sp:where rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >moduleType</sp:varName>
            </sp:object>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
          <sp:TriplePath>
            <sp:object rdf:resource="http://ontology.techlabs.acn.com/appModel#Module"/>
            <sp:path>
              <sp:ModPath>
                <sp:modMin rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >0</sp:modMin>
                <sp:modMax rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >-2</sp:modMax>
                <sp:subPath rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
              </sp:ModPath>
            </sp:path>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >moduleType</sp:varName>
            </sp:subject>
          </sp:TriplePath>
        </sp:where>
      </sp:Construct>
    </spinmap:rule>
  </rdf:Description>
</rdf:RDF>

